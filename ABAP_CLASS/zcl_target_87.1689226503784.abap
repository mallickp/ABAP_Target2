CLASS ZCL_TECHNICAL_SPEC IMPLEMENTATION.

  METHOD CNV_2010C_FILL_BAPIRET2.
    DATA: LS_BAPIRET2 TYPE TY_BAPIRET2.

    LS_BAPIRET2-TYPE = IV_TYPE.
    LS_BAPIRET2-CL = IV_CL.
    LS_BAPIRET2-NUMBER = IV_NUMBER.
    LS_BAPIRET2-PAR1 = IV_PAR1.
    LS_BAPIRET2-PAR2 = IV_PAR2.
    LS_BAPIRET2-PAR3 = IV_PAR3.
    LS_BAPIRET2-PAR4 = IV_PAR4.
    LS_BAPIRET2-LOG_NO = IV_LOG_NO.
    LS_BAPIRET2-LOG_MSG_NO = IV_LOG_MSG_NO.
    LS_BAPIRET2-PARAMETER = IV_PARAMETER.
    LS_BAPIRET2-ROW = IV_ROW.
    LS_BAPIRET2-FIELD = IV_FIELD.

    ES_RETURN = LS_BAPIRET2.
  ENDMETHOD.

  METHOD CNV_2010C_GET_DOMAIN_FIXED_VAL.
    SELECT FIXED_VALUES
      INTO TABLE ET_DOMAIN_FIXED_VAL
      FROM DD07L
      WHERE DOMNAME = IV_DOMNAME.

    RETURN-EV_DOMNAME = IV_DOMNAME.
  ENDMETHOD.

ENDCLASS.